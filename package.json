{
  "name": "dt-sql-parser",
  "version": "4.0.0-beta.3",
  "description": "SQL Parsers for BigData, built with antlr4",
  "keywords": [
    "sql",
    "parser",
    "monaco-editor",
    "hive",
    "spark",
    "flink",
    "bigdata"
  ],
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
